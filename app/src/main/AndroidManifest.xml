<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.carlosisc.pokedex">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo_application"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo_application_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Pokedex">
        <activity android:name="app.carlosisc.pokedex.view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>